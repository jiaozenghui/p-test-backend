{
  "name": "@p-test-backend/server",
  "version": "1.0.8",
  "description": "p-test-cli backend server",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-p-test-server",
    "stop": "egg-scripts stop --title=egg-server-p-test-server",
    "dev": "egg-bin dev",
    "test:local": "egg-bin test -p",
    "test": "npm run lint -- --fix && npm run test:local",
    "cov": "egg-bin cov -p",
    "ci": "npm run lint && npm run cov && npm run tsc && npm run clean",
    "lint": "eslint . --ext .ts --cache",
    "tsc": "tsc",
    "clean": "tsc -b --clean",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "upload": "node webpack/uploadToOSS.js",
    "release": "release-it"
  },
  "dependencies": {
    "@alicloud/dysmsapi20170525": "3.0.0",
    "@alicloud/openapi-client": "^0.4.11",
    "@casl/ability": "^6.7.1",
    "@eggjs/tegg": "^3.5.2",
    "@eggjs/tegg-aop-plugin": "^3.5.2",
    "@eggjs/tegg-config": "^3.2.3",
    "@eggjs/tegg-controller-plugin": "^3.5.2",
    "@eggjs/tegg-eventbus-plugin": "^3.5.2",
    "@eggjs/tegg-plugin": "^3.5.2",
    "@eggjs/tegg-schedule-plugin": "^3.5.2",
    "busboy": "^1.6.0",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "egg": "^3.15.0",
    "egg-bcrypt": "^1.1.0",
    "egg-cors": "latest",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^4.0.1",
    "egg-multipart": "^3.4.0",
    "egg-oss": "^3.1.0",
    "egg-redis": "^2.6.0",
    "egg-scripts": "^2.17.0",
    "egg-static": "^2.3.1",
    "egg-tracer": "^2.0.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "lego-components": "^0.1.7",
    "mongoose-sequence": "^6.0.1",
    "nanoid": "^3.3.7",
    "sharp": "^0.33.5",
    "stream-wormhole": "^2.0.1"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "1",
    "@types/ali-oss": "^6.16.11",
    "@types/mocha": "10",
    "@types/node": "20",
    "ali-oss": "^6.21.0",
    "clean-webpack-plugin": "^4.0.0",
    "egg-bin": "6",
    "egg-mock": "5",
    "eslint": "8",
    "eslint-config-egg": "13",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "release-it": "^17.6.0",
    "typescript": "5",
    "vue": "^3.5.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "repository": {
    "type": "git",
